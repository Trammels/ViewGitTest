<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="textColor" format="color"></attr>
    <attr name="textSize" format="dimension"></attr>
    <declare-styleable name="XCRoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="innerRoundColor" format="color" />
        <attr name="textColor" />
        <attr name="textSize" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <declare-styleable name="custom_view">
        <attr name="videoid" format="integer" />
        <attr name="deletesrc" format="reference" />
        <attr name="text" format="string" />
        <attr name="textColor" />
        <attr name="textSize" />
        <attr name="camerasrc" format="reference" />
        <attr name="videobackgroud" format="reference" />
    </declare-styleable>
</resources>
